{
  "name": "cobra-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:dev": "ng serve --configuration=dev -o",
    "start:uat": "ng serve --configuration=uat",
    "start:qa": "ng serve --configuration=qa",
    "build": "ng build --configuration=production --output-hashing all",
    "build:prod": "ng build --configuration=production --output-hashing all --output-path ../tmp/dist-prod",
    "build:dev": "ng build --output-hashing all --configuration=dev",
    "build:uat": "ng build --output-hashing all --configuration=uat --output-path ../tmp/dist-uat",
    "build:uat:deploy": "ng build --output-hashing all --configuration=uat",
    "build:qa": "ng build --output-hashing all --configuration=qa",
    "build:qa-test": "ng build --output-hashing all --configuration=qa-test",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc --properties es2015 module main --first-only",
    "postinstall": "npm run ngcc"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.8",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.3.8",
    "@angular/compiler": "^13.3.8",
    "@angular/core": "^13.3.8",
    "@angular/flex-layout": "^13.0.0-beta.38",
    "@angular/forms": "^13.3.8",
    "@angular/material": "^13.3.9",
    "@angular/material-moment-adapter": "^13.3.7",
    "@angular/platform-browser": "^13.3.8",
    "@angular/platform-browser-dynamic": "^13.3.8",
    "@angular/router": "^13.3.8",
    "@fortawesome/angular-fontawesome": "^0.10.2",
    "@fortawesome/fontawesome-svg-core": "^6.1.1",
    "@fortawesome/free-solid-svg-icons": "^6.1.1",
    "@kolkov/angular-editor": "^2.0.0",
    "@microsoft/signalr": "^6.0.10",
    "@ngrx/effects": "^13.2.0",
    "@ngrx/store": "^13.2.0",
    "@ngrx/store-devtools": "^13.2.0",
    "es-abstract": "^1.20.1",
    "google-libphonenumber": "^3.2.30",
    "intl-tel-input": "^17.0.3",
    "jwt-decode": "^3.1.2",
    "lodash": "^4.17.21",
    "mat-select-filter": "^2.4.1",
    "moment": "^2.29.3",
    "ngx-bootstrap": "^8.0.0",
    "ngx-cookie-service": "^13.2.1",
    "ngx-intl-tel-input": "^3.2.0",
    "ngx-mask": "^13.1.13",
    "ngx-ui-tour-md-menu": "^8.1.2",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "xlsx": "^0.18.5",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.5",
    "@angular-eslint/builder": "13.2.1",
    "@angular-eslint/eslint-plugin": "13.2.1",
    "@angular-eslint/eslint-plugin-template": "13.2.1",
    "@angular-eslint/schematics": "13.2.1",
    "@angular-eslint/template-parser": "13.2.1",
    "@angular-material-components/datetime-picker": "^7.0.1",
    "@angular-material-components/moment-adapter": "^7.0.0",
    "@angular/cli": "^13.3.5",
    "@angular/compiler-cli": "^13.3.8",
    "@angular/language-service": "^13.3.8",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "^2.0.10",
